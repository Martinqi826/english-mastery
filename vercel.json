{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "public": true,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://english-mastery-production.up.railway.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
